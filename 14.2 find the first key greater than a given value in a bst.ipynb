   "source": [
    "def find_first_greater_than_k(tree, k):\n",
    "    first = None\n",
    "    current = tree\n",
    "\n",
    "    while current:\n",
    "        if k < current.data:\n",
    "            first = current\n",
    "            current = current.left\n",
    "        else:\n",
    "            current = current.right\n",
    "\n",
    "    return first"
   ]
  }
