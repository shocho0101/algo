    "def bsearch(key: int, a: [int]):\n",
    "    b, u = 0, len(a) - 1\n",
    "    while b <= u:\n",
    "        m = b + (u - b) // 2\n",
    "        if a[m] < key:\n",
    "            b = m + 1\n",
    "        elif a[m] == key:\n",
    "            return m\n",
    "        else:\n",
    "            u = m - 1\n",
    "    return -1"
   ]
  }
