   "source": [
    "from linkedlist import ListNode\n",
    "\n",
    "def remove_kth_last(l: ListNode, k: int):\n",
    "    fast = slow = ListNode(0, l)\n",
    "    for _ in range(k):\n",
    "        fast = fast.next\n",
    "\n",
    "    while fast.next:\n",
    "        fast, slow = fast.next, slow.next\n",
    "\n",
    "    slow.next = slow.next.next\n"
   ]
  }
