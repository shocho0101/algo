   "source": [
    "def reverse(x: int) -> int:\n",
    "    result, abs_x = 0, abs(x)\n",
    "    sign = x // abs_x\n",
    "    while abs_x:\n",
    "        result = result * 10 + abs_x % 10\n",
    "        abs_x //= 10\n",
    "    return  result * sign\n",
    "\n",
    "reverse(-655)"
   ]
  }
