   "source": [
    "def square_root(k: int) -> int:\n",
    "    b, u, result = 0, k, 0\n",
    "\n",
    "    if k == 1:\n",
    "        return 1\n",
    "\n",
    "    while b <= u:\n",
    "        m = (b + u) // 2\n",
    "        if m**2 <= k:\n",
    "            result = m\n",
    "            b = m + 1\n",
    "        else:\n",
    "            u = m - 1\n",
    "\n",
    "    return result\n",
    "\n",
    "square_root(200)"
   ]
  }
 ],
